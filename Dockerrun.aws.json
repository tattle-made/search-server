{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
            "name": "search-server",
            "essential": true,
            "image": "tattletech/test-search-server:eb77c2",
            "mountPoints": [
                {
                    "containerPath": "/db-data",
                    "sourceVolume": "DbVolume"
                }
            ],
            "memory": 128,
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80
                }
            ]
        },
        {
            "essential": true,
            "image": "nouchka/sqlite3:latest",
            "name": "word2vec_db"
        }
    ],
    "family": "",
    "volumes": [
        {
            "host": {
                "sourcePath": "/var/app/current/word2vec/db"
            },
            "name": "DbVolume"
        }
    ]
}
